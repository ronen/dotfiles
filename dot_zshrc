HISTFILE=~/.histfile
HISTSIZE=1000
SAVEHIST=1000
zstyle :compinstall filename '/home/ronen/.zshrc'
autoload -Uz compinit
compinit

source ~/.alias

if [ -x $(command -v systemd-path) ]; then
   PATH="$(systemd-path user-binaries):${PATH}"
else
   PATH="$HOME/.local/bin:${PATH}"
fi

if [ -x $(command -v starship) ]; then
    eval "$(starship init zsh)"
fi
